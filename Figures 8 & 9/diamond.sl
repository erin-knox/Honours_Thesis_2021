#!/bin/bash
#SBATCH -J Gem_dia
#SBATCH -A nesi00458
#SBATCH --time=3-00:00:00     
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=20
#SBATCH --mem=30000  

############## USER INPUTS ##############

# load in modules
module load DIAMOND 


#Enter the Diamond database pathways (do not include .dmnd)
DB=/nesi/nobackup/nesi00458/Erin/Diamond/ORF_db


##FILE PATHWAYS
IN=/nesi/nobackup/nesi00458/Erin/Diamond/ORF
OUT=/nesi/nobackup/nesi00458/Erin/Diamond/Output


#Intermediate blast directory
mkdir $OUT/blast
BLAST=$OUT/blast

#Output directory
mkdir $OUT/Results
RESULT=$OUT/Results
mkdir $RESULT/Merge
MERGE=$RESULT/Merge



####### Diamond Blast #######

cd $IN

for f in *.fastq.gz
do 
diamond blastx -d $DB -q $f -a $BLAST/${f}_reads -k 1
done

cd $BLAST

for f in *.daa
do 
diamond view -a $f -o ${f}.m8
done



####### FILTER RESULTS #######
##Parameters for filtering and summarising the Diamond output
#       >= 90% sequence identity ($3)
#       >= 40 amino acid length ($4)

#Set-up sample number variable; combine outputs for sample # (if multiple lanes and/or For/Rev separate) into a single file 
##CHANGE AWK ARGUMENT DEPENDING ON FILE NAME###
for f in *.m8
do
a=$(echo $f|awk -F '[_]' '{print $1"_"$2}')
cat $f | awk '{if ($3 >= 90 && $4 >= 40) print $0}'|cut -f 2 >> $a.txt
done

#Create summary from combined file
for r in *.txt
do
less $r|sort|uniq -c|sort -n -k 1 > $RESULT/${r}_summary.txt
done


#Format output text files for 'join' command
cd $RESULT

for f in *.txt
do
a=$(echo $f|awk -F '[.]' '{print $1}')
cat $f|awk '{print $2,"\t",$1}'|sort > $MERGE/$a.txt
done

####might be a good idea for me to do the joining separate as can run diamond whilst doing the manual coding for the merge####



